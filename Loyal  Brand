    "purchasse_count = df.groupby('user_id')['event_type'].apply(lambda x: (x=='purchase').sum()).fillna(0)\n",
    "user_counts = purchase_counts.value_counts()\n",
    "\n",
    "cumulative_user_counts = user_counts.cumsum()\n",
    "\n",
    "plt.plot(cumulative_user_counts.index, cumulative_user_counts.values)\n",
    "plt.xlabel('Number of Purchases')\n",
    "plt.ylabel('Cumulative Number of Users')\n",
    "plt.title('Cumulative Distribution of Users by Number of Purchases')\n",
