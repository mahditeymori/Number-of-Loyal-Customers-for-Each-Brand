    }
   ],
   "source": [
    "purchase_df = df[df['event_type'] == 'purchase']\n",
    "purchase_df = purchase_df.sort_values(by=['user_id', 'event_time'])\n",
    "first_purchases = purchase_df.groupby('user_id').first().reset_index()\n",
    "purchase_counts = purchase_df.groupby(['user_id', 'brand']).size().reset_index(name='purchase_count')\n",
    "first_purchases = first_purchases.merge(purchase_counts, on=['user_id', 'brand'], how='left')\n",
    "first_purchases['loyal'] = first_purchases['purchase_count'] > 1\n",
    "loyal_customers = first_purchases[first_purchases['loyal'] == True]\n",
    "percent_loyal_customers = len(loyal_customers) / len(first_purchases) * 100\n",
    "print(\"Number of Loyal Customers for Each Brand:\")\n",
    "print(len(loyal_customers))\n",
    "\n",
    "print(\"Number of Loyal Customers for Each Brand:\")\n",
    "print(percent_loyal_customers)"
   ]
  },
